<div class="col-md-8">
  <article class="blog-post">
    <% if (locals.post) { %>
      <h1 class="mb-3"><%= post.title %></h1>
          <div class="mb-4">
          </div>
          <p><%= post.content %></p>

    <% } else { %>
      <h1 class="mb-3">404</h1>
      <p>Sorry, the blog post you are looking for does not exist.</p> 
    <% } %>
  </article>
</div>
